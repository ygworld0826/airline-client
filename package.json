    "@testing-library/react": "^11.2.6",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "nock": "^13.0.11",
    "node-fetch": "^2.6.1",
    "resq": "^1.10.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
